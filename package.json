    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.1",
    "nodemon": "^2.0.12"
  },
  "engines": {
    "node": "14.15.1",
    "npm": "7.20.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Furqanalimir/expense-tracker.git"
  },
